{
    "manifest_version": 2,
    "version": "0.2",
    "name": "Google+ Community Moderating Filter",
    "description": "A simple extension that when enabled, shows only posts that needs moderating.",
    "permissions": [ "tabs", "http://plus.google.com/" ],
    "icons": {
    	"16": "img/icon16.png",
    	"48": "img/icon48.png",
    	"128": "img/icon128.png"
    },
    "browser_action": {
        "default_icon": "img/icon48.png",
        "default_popup": "main.html"
    },
    "content_scripts": [
        {
            "matches": ["*://plus.google.com/*"],
            "js": ["/js/jquery-1.7.2.min.js", "/js/content_scripts/content_script.js"]
        }
    ]
}
